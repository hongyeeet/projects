import time as t
from tkinter import *
import threading 
root = Tk()
root.title('password game')
root.geometry('500x600')


def getpw():
    global pw
    pw = password.get()
    print(pw)
    if CHAR.count(pw) < 5:
        print('lol')




password = Entry(root,width =35)
password.place(x=150,y=50)
Label(text='Password:').place(x=90,y=48)
submit = Button(root,text='submit',command=getpw)
submit.place(x=370,y=47)



 

















root.mainloop()